                <inertia>
                    <ixx>6.96908333e-07</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>6.96908333e-07</iyy>
                    <iyz>0</iyz>
                    <izz>1.125e-06</izz>
